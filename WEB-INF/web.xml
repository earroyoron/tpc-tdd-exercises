<?xml version="1.0" encoding="ISO-8859-1"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   version="2.5">

  <display-name>Test Driven Development Exercises &amp; Examples</display-name>

  <!-- Página de error de recurso no encontrado para los ejercicios que
       no contienen algún tipo de informe y está referenciado en los shortcuts. -->
  <error-page>
    <error-code>404</error-code>
    <location>/noreportredirect.jsp</location>
  </error-page>

</web-app>